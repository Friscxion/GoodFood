generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Plat {
  id               Int     @id @default(autoincrement())
  prix_ttc         Float
  list_ingredient  String[]
  nom              String  @db.VarChar(255)
  restaurant_id    Int?
  image            String?
  cuisine          String?
  tags             String[]
  supplements      String[]
  description      String?
}
